#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here



puts "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " " ]
display_board(board)


puts "Where would you like to go?"

index = gets.strip 


index = input_to_index(index)

move(board, index, value = "X")

display_board(board)

